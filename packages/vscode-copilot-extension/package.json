      },
      {
        "command": "ms-copilot-extension.validateManifest",
        "title": "%teamstoolkit.commands.validateManifest.title%",
        "enablement": "ms-copilot-extension.isTeamsFx && isWorkspaceTrusted",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.azureAccountSettings",
        "title": "%teamstoolkit.commands.azureAccountSettings.title%",
        "icon": "$(settings-gear)"
      },
      {
        "command": "ms-copilot-extension.createAccount",
        "title": "%teamstoolkit.commands.createAccount.title%",
