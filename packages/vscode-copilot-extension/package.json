        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.updateAadAppManifest",
        "title": "%teamstoolkit.commands.updateAadAppManifest.title%",
        "enablement": "ms-copilot-extension.isTeamsFx && ms-copilot-extension.isAadManifestEnabled && isWorkspaceTrusted",
        "category": "Teams"
