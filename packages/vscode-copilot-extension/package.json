{
  "name": "ms-copilot-vscode-extension",
  "displayName": "Teams Toolkit",
  "description": "Create, debug, and deploy Teams apps with Teams Toolkit",
  "version": "5.0.1",
  "publisher": "TeamsDevApp",
  "author": "Microsoft Corporation",
  "private": true,
  "icon": "media/github-copilot.png",
  "main": "./out/src/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/TeamsFx"
  },
  "views": {
    "ms-copilot-toolkit": [
      {
        "id": "ms-copilot-toolkit",
        "name": "Commands"
      }
    ]
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "license": "MIT",
  "keywords": [
    "Microsoft Teams",
    "Azure",
    "Microsoft 365"
  ],
  "preview": false,
  "categories": [
    "Azure",
    "Extension Packs",
    "Snippets"
  ],
  "aiKey": "1c56be97-bb74-42cf-b04b-8f1aabf04592",
  "featureFlag": "true",
  "files": [
    "out/**/*"
  ],
  "activationEvents": [
    "onCommand:ms-copilot-extension.create",
    "onCommand:ms-copilot-extension.createFromWalkthrough",
    "onCommand:ms-copilot-extension.openLifecycleTreeview",
    "onCommand:ms-copilot-extension.openDocument",
    "onCommand:ms-copilot-extension.openReadMe",
    "onCommand:ms-copilot-extension.openSamples",
    "onCommand:ms-copilot-extension.openWelcome",
    "onCommand:ms-copilot-extension.selectAndDebug",
    "onCommand:ms-copilot-extension.selectTutorials",
    "onCommand:ms-copilot-extension.signinM365",
    "onCommand:ms-copilot-extension.validate-getStarted-prerequisites",
    "onCommand:workbench.action.tasks.runTask",
    "onCommand:workbench.action.debug.start",
    "workspaceContains:/.fx/**/*",
    "workspaceContains:teamsapp.yml",
    "onView:ms-copilot-empty-project",
    "onUri"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "%teamstoolkit.capabilities.untrustedWorkspaces.description%",
      "restrictedConfigurations": []
    }
  },
  "contributes": {
    "icons": {
      "ms-copilot-add-feature": {
        "description": "Add feature icon in command tree view",
        "default": {
          "fontPath": "./media/font/teamstoolkit.woff",
          "fontCharacter": "\\E000"
        }
      },
      "ms-copilot-developer-portal": {
        "description": "Developer portal icon in command tree view",
        "default": {
          "fontPath": "./media/font/teamstoolkit.woff",
          "fontCharacter": "\\E001"
        }
      },
      "ms-copilot-m365": {
        "description": "Microsoft 365 icon in command tree view",
        "default": {
          "fontPath": "./media/font/teamstoolkit.woff",
          "fontCharacter": "\\E002"
        }
      },
      "ms-copilot-add-environment": {
        "description": "Microsoft 365 icon in command tree view",
        "default": {
          "fontPath": "./media/font/teamstoolkit.woff",
          "fontCharacter": "\\E004"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ms-copilot",
          "title": "Copilot plugin extension",
          "icon": "media/github-copilot.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "ms-copilot-empty-project",
        "contents": "%teamstoolkit.viewsWelcome.ms-copilot-empty-project.content%",
        "enablement": "ms-copilot-extension.initialized"
      },
      {
        "view": "ms-copilot-empty-project-new-user",
        "contents": "%teamstoolkit.viewsWelcome.ms-copilot-empty-project-new-user.content%",
        "enablement": "ms-copilot-extension.initialized"
      },
      {
        "view": "ms-copilot-project-and-check-upgradeV3",
        "contents": "%teamstoolkit.viewsWelcome.ms-copilot-project-and-check-upgradeV3.content%",
        "enablement": "ms-copilot-extension.initialized"
      },
      {
        "view": "ms-copilot-feedback",
        "contents": "%teamstoolkit.viewsWelcome.ms-copilot-feedback.content%",
        "enablement": "ms-copilot-extension.initialized"
      }
    ],
    "views": {
      "ms-copilot": [
        {
          "id": "ms-copilot-accounts",
          "name": "Accounts",
          "when": "ms-copilot-extension.isTeamsFx"
        },
        {
          "id": "ms-copilot-environment",
          "name": "Environment",
          "when": "ms-copilot-extension.isTeamsFx && !ms-copilot-extension.canUpgradeV3 && isWorkspaceTrusted"
        },
        {
          "id": "ms-copilot-development",
          "name": "Development",
          "when": "ms-copilot-extension.isTeamsFx"
        },
        {
          "id": "ms-copilot-lifecycle",
          "name": "Lifecycle",
          "when": "ms-copilot-extension.isTeamsFx && !ms-copilot-extension.canUpgradeV3"
        },
        {
          "id": "ms-copilot-utility",
          "name": "Utility",
          "when": "ms-copilot-extension.isTeamsFx && !ms-copilot-extension.canUpgradeV3"
        },
        {
          "id": "ms-copilot-project-and-check-upgradeV3",
          "name": "Upgrade",
          "when": "ms-copilot-extension.isTeamsFx && ms-copilot-extension.canUpgradeV3 && isWorkspaceTrusted"
        },
        {
          "id": "ms-copilot-help-and-feedback",
          "name": "Help and feedback",
          "when": "ms-copilot-extension.isTeamsFx && !ms-copilot-extension.canUpgradeV3",
          "visibility": "visible"
        },
        {
          "id": "ms-copilot-help",
          "name": "Help",
          "when": "false",
          "visibility": "collapsed"
        },
        {
          "id": "ms-copilot-feedback",
          "name": "Feedback",
          "when": "false",
          "visibility": "visible"
        },
        {
          "id": "ms-copilot-empty-project",
          "name": "Teams Toolkit",
          "when": "!ms-copilot-extension.isTeamsFx && !ms-copilot-extension.isNewUser"
        },
        {
          "id": "ms-copilot-empty-project-new-user",
          "name": "Teams Toolkit",
          "when": "!ms-copilot-extension.isTeamsFx && ms-copilot-extension.isNewUser"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "ms-copilot-extension.addEnvironmentWithIcon",
          "when": "view == ms-copilot-environment",
          "group": "navigation@1"
        },
        {
          "command": "ms-copilot-extension.refreshEnvironment",
          "when": "view == ms-copilot-environment",
          "group": "navigation@2"
        },
        {
          "command": "ms-copilot-extension.openAccountLink",
          "when": "view == ms-copilot-accounts",
          "group": "navigation@2"
        },
        {
          "command": "ms-copilot-extension.createAccount",
          "when": "view == ms-copilot-accounts",
          "group": "navigation@1"
        },
        {
          "command": "ms-copilot-extension.openEnvLink",
          "when": "view == ms-copilot-environment",
          "group": "navigation@3"
        },
        {
          "command": "ms-copilot-extension.openDevelopmentLink",
          "when": "view == ms-copilot-development",
          "group": "navigation@1"
        },
        {
          "command": "ms-copilot-extension.openLifecycleLink",
          "when": "view == ms-copilot-lifecycle",
          "group": "navigation@1"
        },
        {
          "command": "ms-copilot-extension.openHelpFeedbackLink",
          "when": "view == ms-copilot-help-and-feedback",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "ms-copilot-extension.signOut",
          "when": "view == ms-copilot-accounts && viewItem == signedinM365",
          "group": "inline@1"
        },
        {
          "command": "ms-copilot-extension.signOut",
          "when": "view == ms-copilot-accounts && viewItem == signedinAzure",
          "group": "inline@1"
        },
        {
          "command": "ms-copilot-extension.m365AccountSettings",
          "when": "view == ms-copilot-accounts && viewItem == signedinM365",
          "group": "inline@2"
        },
        {
          "command": "ms-copilot-extension.refreshSideloading",
          "when": "view == ms-copilot-accounts && viewItem == checkSideloading",
          "group": "inline"
        },
        {
          "command": "ms-copilot-extension.azureAccountSettings",
          "when": "view == ms-copilot-accounts && viewItem == signedinAzure",
          "group": "inline@2"
        },
        {
          "command": "ms-copilot-extension.openDocumentLink",
          "when": "view == ms-copilot-accounts && viewItem =~ /^signinM365$|^signinAzure$/",
          "group": "inline"
        },
        {
          "command": "ms-copilot-extension.preview",
          "when": "view == ms-copilot-environment && viewItem =~ /environment-provisioned|local-existing-app/"
        },
        {
          "command": "ms-copilot-extension.previewWithIcon",
          "when": "view == ms-copilot-environment && viewItem =~ /environment-provisioned|local-existing-app/",
          "group": "inline@3"
        },
        {
          "command": "ms-copilot-extension.localdebug",
          "when": "view == ms-copilot-environment && viewItem =~ /^local$/"
        },
        {
          "command": "ms-copilot-extension.localdebugWithIcon",
          "when": "view == ms-copilot-environment && viewItem =~ /^local$/",
          "group": "inline@3"
        },
        {
          "command": "ms-copilot-extension.manageCollaborator",
          "when": "view == ms-copilot-environment && viewItem =~ /environment/",
          "group": "inline@3"
        },
        {
          "command": "ms-copilot-extension.openSubscriptionInPortal",
          "when": "view == ms-copilot-environment && viewItem == openSubscriptionInPortal",
          "group": "inline@3"
        },
        {
          "command": "ms-copilot-extension.openResourceGroupInPortal",
          "when": "view == ms-copilot-environment && viewItem == openResourceGroupInPortal",
          "group": "inline@3"
        },
        {
          "command": "ms-copilot-extension.openDocumentLink",
          "when": "view == ms-copilot-development && viewItem =~ /^ms-copilot-extension.(create|openSamples|openManifest)$/",
          "group": "inline"
        },
        {
          "command": "ms-copilot-extension.openDocumentLink",
          "when": "view == ms-copilot-lifecycle && viewItem =~ /^ms-copilot-extension.(provision|deploy|publish)$/",
          "group": "inline"
        },
        {
          "command": "ms-copilot-extension.openDocumentLink",
          "when": "view == ms-copilot-utility && viewItem =~ /^ms-copilot-extension.(build|publishInDeveloperPortal)$/",
          "group": "inline"
        }
      ],
      "editor/title/run": [
        {
          "command": "ms-copilot-extension.selectAndDebug",
          "when": "!inDebugMode && debuggersAvailable && ms-copilot-extension.runIconActive",
          "group": "teamsfx"
        }
      ],
      "editor/title": [
        {
          "command": "ms-copilot-extension.openPreviewAadFile",
          "when": "resourceFilename == aad.template.json && ms-copilot-extension.isAadManifestEnabled",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "ms-copilot-extension.addEnvironmentWithIcon",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.azureAccountSettings",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.createAccount",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.deployManifestFromCtxMenu",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.grantPermission",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.listCollaborator",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.manageCollaborator",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.localdebug",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.localdebugWithIcon",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.m365AccountSettings",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openAccountLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openLifecycleLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openDevelopmentLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openDocumentLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openEnvLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openHelpFeedbackLink",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openPreviewAadFile",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openResourceGroupInPortal",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openSchema",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.openSubscriptionInPortal",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.preview",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.previewWithIcon",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.refreshEnvironment",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.refreshSideloading",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.selectAndDebug",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.signOut",
          "when": "false"
        },
        {
          "command": "ms-copilot-extension.checkProjectUpgrade",
          "when": "ms-copilot-extension.canUpgradeV3"
        },
        {
          "command": "ms-copilot-extension.openSurvey",
          "when": "false"
        }
      ]
    },
    "commands": [
      {
        "command": "ms-copilot-extension.create",
        "title": "%teamstoolkit.commands.createProject.title%",
        "enablement": "!ms-copilot-extension.commandLocked",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.createFromWalkthrough",
        "title": "Teams - Create Project",
        "enablement": "never"
      },
      {
        "command": "ms-copilot-extension.openLifecycleTreeview",
        "title": "Teams - Open Lifecycle Treeview",
        "enablement": "never"
      },
      {
        "command": "ms-copilot-extension.openDocument",
        "title": "%teamstoolkit.commands.document.title%",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.openReadMe",
        "title": "Teams - Open Read Me",
        "enablement": "never"
      },
      {
        "command": "ms-copilot-extension.openSamples",
        "title": "%teamstoolkit.commands.viewSamples.title%",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.openWelcome",
        "title": "%teamstoolkit.commands.getstarted.title%",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.selectAndDebug",
        "title": "%teamstoolkit.commands.debug.title%",
        "icon": "$(run)"
      },
      {
        "command": "ms-copilot-extension.selectTutorials",
        "title": "%teamstoolkit.commands.selectTutorials.title%",
        "icon": "$(tasklist)",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.validate-getStarted-prerequisites",
        "title": "Teams - Validate Get Started Prerequisites",
        "enablement": "never"
      },
      {
        "command": "ms-copilot-extension.addEnvironment",
        "title": "%teamstoolkit.commands.createEnvironment.title%",
        "enablement": "ms-copilot-extension.isTeamsFx && isWorkspaceTrusted && !ms-copilot-extension.commandLocked",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.addEnvironmentWithIcon",
        "title": "%teamstoolkit.commands.createEnvironment.title%",
        "icon": "$(add)",
        "enablement": "ms-copilot-extension.isTeamsFx && isWorkspaceTrusted && !ms-copilot-extension.commandLocked"
      },
      {
        "command": "ms-copilot-extension.build",
        "title": "%teamstoolkit.commands.zipPackage.title%",
        "enablement": "ms-copilot-extension.isTeamsFx && isWorkspaceTrusted && !ms-copilot-extension.commandLocked",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.checkProjectUpgrade",
        "title": "%teamstoolkit.commands.upgradeProject.title%",
        "enablement": "ms-copilot-extension.initialized && isWorkspaceTrusted",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.cmpAccounts",
        "title": "%teamstoolkit.commands.accounts.title%",
        "category": "Teams",
        "enablement": "ms-copilot-extension.initialized"
      },
      {
        "command": "ms-copilot-extension.deploy",
        "title": "%teamstoolkit.commands.deploy.title%",
