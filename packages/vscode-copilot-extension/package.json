        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.publishInDeveloperPortal",
        "title": "%teamstoolkit.commandsTreeViewProvider.publishInDevPortalTitle%",
        "enablement": "ms-copilot-extension.isTeamsFx && isWorkspaceTrusted && ms-copilot-extension.isTDPIntegrationEnabled && !ms-copilot-extension.commandLocked",
        "category": "Teams"
      },
      {
        "command": "ms-copilot-extension.openReportIssues",
        "title": "%teamstoolkit.commands.reportIssue.title%",
        "enablement": "ms-copilot-extension.isTeamsFx",
        "category": "Teams"
